    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^3.9.10"
